// Steep parallax occlusion mapping
material RTSS/SteepParallaxOcclusionMapping
{
   technique
   {
       pass 
       {
           specular 1 1 1 32

           // Base diffuse texture map 
           texture_unit 
           { 
              texture rockwall.tga
           }

           rtshader_system
           {
               lighting_stage normal_map rockwall_NH.tga parallax_steep

           }
       }
   }
}